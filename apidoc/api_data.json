      "fields": {
        "Success 200": [
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "characterImg",
            "description": "<p>인증 팝업창에 나타날 캐릭터 이미지</p>"
          },
          {
            "group": "Success 200",
            "type": "Object",
            "optional": false,
            "field": "challengeCompletion",
            "description": "<p>챌린지 성공 응답 객체</p>"
